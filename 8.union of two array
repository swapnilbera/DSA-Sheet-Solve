import java.util.*;
public class union {
    public static void main(String[] args) {
        int arr1[]={7,3,9};
        int arr2[]={7,3,9,6,2,4};
        HashSet<Integer> map=new HashSet<>();
        for(int i=0;i<arr1.length;i++)
             map.add(arr1[i]);
        for(int i=0;i<arr2.length;i++)
            map.add(arr2[i]);
        System.out.println(map.size());
    }
}
